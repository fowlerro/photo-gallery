{
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"printWidth": 120,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js"],
			"options": {
				"parser": "typescript"
			}
		}
	]
}
